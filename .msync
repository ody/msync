---
:disk: Archive
:destination: /Volumes/Archive/user/Sites/mirrors
:source: mirrors.cat.pdx.edu
:mtype:
  :apt:
    :staging: /Volumes/Archive/user/apt-mirror
    :syncer:
      :apt-mirror:
        :mirrors:
          :"debian-backports":
            :releases:
              - squeeze-backports
          :debian:
            :releases:
              - squeeze
              - squeeze-updates
              - wheezy
  :rpm:
    :staging: /Volumes/Archive/user/rsync-mirror
    :syncer:
      :rsync:
        :mirrors:
          :centos:
            :releases:
              - 5
              - 6
          :fedora-epel:
            :releases:
              - 5
              - 6
